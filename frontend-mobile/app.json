    "slug": "frontend-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/LogoSR.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
        "image": "./assets/LogoSR.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
